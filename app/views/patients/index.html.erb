
<h2><span class ="SelectedTab">TRIAGE TAB</span>   <span class ="UnselectedTab"><%= link_to "AMBULANCE TAB", incident_ambulances_path(@incident) %></span>     <span class ="UnselectedTab"><%= link_to "CLOSE TAB", close_incident_path(@incident) %></span></h2>
<div id="incident_info" data-id="<%= @incident.id %>"></div>
<% if current_user %>
  <% if User.find(current_user.id).role_id == 1 %>
    <div class="incident" id="patient_input" data-id="<%= @incident.id %>">
      <br />
      <%= render 'form' %>
      <br />
    </div>
    <hr />
    <center><span class="highlight"<a class="min_toggle"id="min_patient_form" >PATIENT FORM</a></span></center>
  <% end %>
<% end %>
	<div id="patient-notice"></div>
	<br />


	<div class="scrollable2">
		<div class="patientsTable">
			<table id="tableForPatients" class="tablesorter">
				<thead>
				<tr>
		    		<th>TAG#</th>
		    		<th>COLOR</th>
		    		<th>SEX</th>
		    		<th>AGE</th>
		    		<th></th>
		    		<th>COMPLAINT(S)</th>
					<th>STATUS</th>
					<th>HOSPITAL</th>
					<th>AMBULANCE</th>
					<th>DELETE</th>
				</tr>
				</thead>		
				<tbody id="tbody_created_patients" class="tbody_created_patients">

                                  <%= render :partial => @patients, :locals => {:list => true} %>
                                  
				</tbody>
			</table>
		</div>
	
</div>
        
<br />

