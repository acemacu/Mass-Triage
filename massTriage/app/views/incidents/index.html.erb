<h1>Listing incidents</h1>

<table>
  <tr>
    <th>Idincident</th>
    <th>Date</th>
    <th>Type</th>
    <th>Location</th>
    <th>Longitude</th>
    <th>Latitude</th>
    <th>Dispatchtime</th>
    <th>Arrivaltime</th>
    <th>Endtime</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @incidents.each do |incident| %>
  <tr>
    <td><%= incident.idIncident %></td>
    <td><%= incident.date %></td>
    <td><%= incident.type %></td>
    <td><%= incident.location %></td>
    <td><%= incident.longitude %></td>
    <td><%= incident.latitude %></td>
    <td><%= incident.dispatchTime %></td>
    <td><%= incident.arrivalTime %></td>
    <td><%= incident.endTime %></td>
    <td><%= link_to 'Show', incident %></td>
    <td><%= link_to 'Edit', edit_incident_path(incident) %></td>
    <td><%= link_to 'Destroy', incident, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Incident', new_incident_path %>
